#define ODD(_NAME, _SRC) \
	group { _NAME; \
		inherit: _SRC; \
		parts { \
			part { "base"; \
				desc { state: "default" 0.0; \
					color_class: "ilist_item_odd_base"; \
				} \
			} \
		} \
	}
